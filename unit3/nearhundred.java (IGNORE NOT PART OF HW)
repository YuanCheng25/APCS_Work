public static boolean nearHundred(int n) {
    if(Math.abs(result) <= 10 || Math.abs(200 - n))
  return true;
    else 
        return false;
}
    
public static void testHundred(int n, int expected)
    result=n;
    
 if(expected==result) 
     System.out.println("pass");
